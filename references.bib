}

@book{reddy_service_2013,
	title = {{SERVICE} {LIFE} {PREDICTION} {MODELS} {FOR} {CONCRETE} {STRUCTURES}-{A} {COMPARATIVE} {STUDY}},
	abstract = {Several models are available to predict the service life of concrete structures. However, not all models are suitable for use in all the cases, especially for the advanced concrete systems exposed to complex environmental conditions. In addition, all the models do not predict similar results due to various technical merits and demerits. This paper provides a comparative study on four existing service life prediction models (i.e., Life-365™, CHLODIF, ClinConc and DuraCrete). The comparison is based on the predicted corrosion initiation time for various conditions. The merits and demerits of these models are also discussed in brief. Then, the sensitivity of these models to various input parameters (e.g., cover depth, chloride diffusion coefficient, and chloride threshold of steel) in predicting the corrosion initiation time is presented.},
