	author = {Lamaa, Ghandy and Duarte, António P. C. and Silva, Rui Vasco and de Brito, Jorge},
	month = jan,
	year = {2023},
	keywords = {CO₂ capture, accelerated carbonation, alkali-activated materials, strength development},
	pages = {3086},
}

