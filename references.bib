@article{mohd_noor_heritage_2019,
	title = {Heritage building condition assessment: a case study from {Johor} {Bahru}, {Malaysia}},
	volume = {220},
	issn = {1755-1315},
	shorttitle = {Heritage building condition assessment},
	url = {https://iopscience.iop.org/article/10.1088/1755-1315/220/1/012024},
	doi = {10.1088/1755-1315/220/1/012024},
	abstract = {Building condition assessment (BCA) is an assessment that provides the information of deficiencies of the property to support the decision-making and management of its maintenance and rehabilitation. This paper presents the execution of BCA on heritage building, namely Balai Zaharah in Johor Bharu to obtain its rating. The BCA was conducted based on the structural and architectural elements’ maintainability, security, functionality and the sustainability of the construction material’s originality. Therefore, the main objective of this study is to identify the rating of each defect according to its condition and maintenance priority. From each defect, the overall rating from 1 to 5 of the building was determined. A systematic recording of the building systems is one of the methods used in this study by visual inspection and technical notes of each defects and deficiencies existed. As for the material suitability, energy-dispersive X-ray spectroscopy (EDX) was performed on the element samples to identify the chemical composition of the construction materials used so that the originality can be sustained during the rehabilitation and maintenance works. In conclusion, from the building assessment and matrix analysis, it was found that Balai Zaharah was rated to require major maintenance and rehabilitation to reestablish to its original state.},
	language = {en},
	urldate = {2023-12-12},
	journal = {IOP Conference Series: Earth and Environmental Science},
	author = {Mohd Noor, Syahirah and Shing Mei, Chiew and Syahrizal Ibrahim, Izni and Nabilah Sarbini, Noor and Hanim Osman, Mohd and Azeyah Khiyon, Noor},
	month = feb,
	year = {2019},
