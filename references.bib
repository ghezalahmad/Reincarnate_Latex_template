	doi = {10.1016/B978-0-12-820512-9.00023-X},
	keywords = {Accelerated aging test, Durability, Fiber reinforced polymer (FRP) composites, In-service environments, Life prediction, Synthetic fibers},
	pages = {179--203},
}

